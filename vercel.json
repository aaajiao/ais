{
  "framework": "vite",
  "functions": {
    "api/export/pdf.ts": {
      "includeFiles": "api/export/fonts/**"
    }
  },
  "rewrites": [
    { "source": "/api/view/:token", "destination": "/api/view/[token]" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
